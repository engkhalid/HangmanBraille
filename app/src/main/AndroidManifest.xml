<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ece558winter.khalidalkhulayfi.hangmanbraille" >


    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PracticeActivity"
            android:label="@string/practice_activity_name">
        </activity>
        <activity
            android:name=".GameActivity"
            android:label="@string/game_activity_name">
        </activity>
        <activity
            android:name=".TestActivity"
            android:label="@string/test_activity_name">
        </activity>

    </application>

</manifest>
